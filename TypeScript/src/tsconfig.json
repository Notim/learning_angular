{
 "compilerOptions": {
    "outDir": "../dist",
    "module": "commonjs",
    "target": "es2015",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "charset": "utf-8",
    "baseUrl": "./",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "classic",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": false,
    "typeRoots": [
        "node_modules/@types"
    ],
    "lib": [
        "es2018",
        "dom"
    ]
 }
}